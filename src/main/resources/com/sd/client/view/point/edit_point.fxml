<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sd.client.view.controllers.EditPointController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="Nome" />
   <TextField fx:id="name_tf" />
    <Label text="Observação" />
   <TextArea fx:id="obs_ta" prefHeight="200.0" prefWidth="200.0" />
    <Button fx:id="edit_btn" onAction="#edit" text="Editar" />
   <Button mnemonicParsing="false" onAction="#back" text="Voltar" />
</VBox>
